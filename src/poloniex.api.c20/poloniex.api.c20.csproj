<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Title>Poloniex RESTFul API</Title>
    <Description>C# Poloniex RESTFul API</Description>
    <Copyright>Copyright OdinSoftware Co., Ltd. 2017</Copyright>
    <AssemblyTitle>Poloniex.API.Core</AssemblyTitle>
    <VersionPrefix>1.0.0</VersionPrefix>
    <Authors>SEONGAHN LEE</Authors>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <AssemblyName>Poloniex.API</AssemblyName>
    <PackageId>Poloniex.API.Core</PackageId>
    <PackageTags>odinsoft;sdk;trading-api;bitcoin;cryptocurrency</PackageTags>
    <PackageIconUrl>https://github.com/lisa3907/poloniex.csharp/blob/master/logo-files/poloniex.api.ico?raw=true</PackageIconUrl>
    <PackageProjectUrl>https://github.com/lisa3907/poloniex.csharp</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/lisa3907/poloniex.csharp/blob/master/LICENSE</PackageLicenseUrl>
    <NetStandardImplicitPackageVersion>1.6.1</NetStandardImplicitPackageVersion>
    <PackageTargetFallback>$(PackageTargetFallback);dnxcore50</PackageTargetFallback>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    <RootNamespace>Poloniex.API</RootNamespace>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Version>2.0.1</Version>
    <AssemblyVersion>2.0.1.0</AssemblyVersion>
    <FileVersion>2.0.1.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Configuration.Abstractions" Version="1.1.2" />
    <PackageReference Include="Microsoft.Win32.Registry" Version="4.3.0" />
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
    <PackageReference Include="OdinSdk.BaseLib.Core" Version="1.0.8" />
    <PackageReference Include="RestSharp.NetCore" Version="105.2.3" />
    <PackageReference Include="System.Net.NameResolution" Version="4.3.0" />
    <PackageReference Include="System.Net.NetworkInformation" Version="4.3.0" />
    <PackageReference Include="System.Runtime.Serialization.Formatters" Version="4.3.0" />
  </ItemGroup>

</Project>
